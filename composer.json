{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "anlutro/l4-settings": "^1.3",
        "barryvdh/laravel-dompdf": "^2.0",
        "barryvdh/laravel-elfinder": "^0.5.2",
        "barryvdh/laravel-httpcache": "^1.0",
        "barryvdh/laravel-translation-manager": "^0.6.4",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.15",
        "laravel/octane": "^1.5",
        "laravel/sanctum": "^3.2",
        "laravel/telescope": "^4.14",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "predis/predis": "^2.1",
        "rybakit/msgpack": "^0.9.1",
        "spatie/backtrace": "^1.4",
        "spatie/db-dumper": "^3.3",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/enum": "^3.13",
        "spatie/ignition": "^1.4",
        "spatie/image": "^2.2",
        "spatie/image-optimizer": "^1.6",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-data": "^3.2",
        "spatie/laravel-fractal": "^6.0",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-permission": "^5.10",
        "spatie/laravel-query-builder": "^5.2",
        "spatie/laravel-translatable": "^6.4",
        "spatie/macroable": "^2.0",
        "spatie/once": "^3.1",
        "spatie/pdf-to-image": "^1.2",
        "spatie/robots-txt": "^2.0",
        "spatie/temporary-directory": "^2.1",
        "spiral/roadrunner": "^2.8.2",
        "tanmuhittin/laravel-google-translate": "^2.2",
        "yajra/laravel-datatables": "^10.0",
        "yusuftogtay/generator-builder": "dev-LG-1-Configuration-Detailing",
        "yusuftogtay/l5-repository": "dev-master",
        "yusuftogtay/laravel-generator": "dev-master"
    },
    "repositories": [
        {
            "type": "path",
            "url": "modules/l5-repository"
        },
        {
            "type": "path",
            "url": "modules/laravel-generator"
        },
        {
            "type": "path",
            "url": "modules/generator-builder"
        }
    ],
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper",
                "laravel/telescope",
                "laravel/horizon"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
